	<subDataset name="criterias" uuid="01865c69-d417-4f42-a83a-e0c4283864be">
		<parameter name="bidId" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="supplierId" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
PQQualificationEvaluations.justification,
PQQualificationEvaluations.result,
PQQualificationEvaluations.evaluationType,
PQQualificationEvaluationCriterias.subjectNo,
PQQualificationEvaluationCriterias.requirement,
PQSubcriteriaCategories.categoryName AS subcategory,
PQCriteriaCategories.categoryName AS category

FROM
PQQualificationEvaluations

INNER JOIN PQQualificationEvaluationCriterias
ON PQQualificationEvaluations.criteria_id= PQQualificationEvaluationCriterias.id

INNER JOIN PQSubcriteriaCategories
ON PQQualificationEvaluationCriterias.subject_id = PQSubcriteriaCategories.id

INNER JOIN PQCriteriaCategories
ON PQSubcriteriaCategories.category_id=PQCriteriaCategories.id

WHERE
(PQQualificationEvaluations.evaluationType='1' AND PQQualificationEvaluationCriterias.bid_id=$P{bidId})
 AND
PQQualificationEvaluations.supplier_id=$P{supplierId}]]>
		</queryString>
		<field name="justification" class="java.lang.String"/>
		<field name="result" class="java.lang.Integer"/>
		<field name="evaluationType" class="java.lang.Integer"/>
		<field name="subjectNo" class="java.lang.String"/>
		<field name="requirement" class="java.lang.String"/>
		<field name="subcategory" class="java.lang.String"/>
		<field name="category" class="java.lang.String"/>
		<variable name="result" class="java.lang.String">
			<variableExpression><![CDATA[( ($F{result}==0) ? "Pass" : "Fail" )]]></variableExpression>
		</variable>
		<variable name="remarks" class="java.lang.String">
			<variableExpression><![CDATA["<p>"+$F{justification}+"</p>"]]></variableExpression>
		</variable>
		<variable name="conclusion" class="java.lang.Integer">
			<variableExpression><![CDATA[$F{result}==0?$V{conclusion}:$V{conclusion}+1]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<variable name="conclusionStatement" class="java.lang.String">
			<variableExpression><![CDATA[$V{conclusion}>0?"FAIL":"PASS"]]></variableExpression>
		</variable>
		<group name="category">
			<groupExpression><![CDATA[$F{category}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="bidId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
